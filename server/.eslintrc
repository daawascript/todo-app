{
  "env": {
    "browser": false,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["eslint-config-airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "never",
        "exports": "never",

        "functions": "never"
      }
    ]
  }
}
